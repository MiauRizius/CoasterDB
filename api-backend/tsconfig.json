{
    "extends": "@tsconfig/node18/tsconfig.json",
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES2020",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  